@article{Kohler2000,
abstract = {Click is a new software architecture for building exible and con gurable routers. A Click router is assembled from packet processing modules called elements. Individual elements implement simple router functions like packet classi cation, queueing, scheduling, and interfacing with network devices. A router con guration is a directed graph with elements at the vertices; packets ow along the edges of the graph. Several features make individual elements more powerful and complex con gurations easier to write, including pull connections, which model packet ow driven by transmitting hardware devices, and ow-based router context, which helps an element locate other interesting elements. Click con gurations are modular and easy to extend. A standards-compliant Click IP router has sixteen elements on its forwarding path; some of its elements are also useful in Ethernet switches and IP tunneling con gurations. Extending the IP router to support dropping policies, fairness among ows, or Differentiated Services simply requires adding a couple elements at the right place. On conventional PC hardware, the Click IP router achieves a maximum loss-free forwarding rate of 333,000 64-byte packets per second, demonstrating that Click's modular and exible architecture is compatible with good performance.},
author = {Kohler, Eddie and Morris, Robert and Chen, Benjie and Jannotti, John and Kaashoek, M. Frans},
doi = {10.1145/354871.354874},
file = {:home/arturo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kohler et al. - 2000 - The click modular router.pdf:pdf},
isbn = {1581131402},
issn = {07342071},
journal = {ACM Transactions on Computer Systems},
number = {212},
pages = {263--297},
title = {{The click modular router}},
volume = {18},
year = {2000}
}
@article{Liu2004,
abstract = {With the popularity of e-learning systems, there is an increasing demand on effectively utilizing instructional videos to augment distance learning experiences. This paper addresses some special issues in processing and streaming lecture videos, and provides a new approach for real-time content analysis and adaptive transmission of these videos over wireless networks. A content-based analysis method and a buffer-based model are provided to detect content regions and to select content significant key frames, and an adaptive feedback control scheme is applied to transmit properly compressed key frames to accommodate dynamically changing bandwidths in wireless environments, with consideration of the limitations of resolution and computing capability of mobile devices. We demonstrate the effectiveness and scalability of our system in experimental results of adaptive transmission of instructional videos over 802.11b wireless networks to mobile devices.},
author = {Liu, Tiecheng and Choudary, Chekuri},
doi = {10.1145/1027527.1027622},
file = {:home/arturo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Liu, Choudary - 2004 - Real-time content analysis and adaptive transmission of lecture videos for mobile applications.pdf:pdf},
isbn = {1581138938},
journal = {ACM Multimedia 2004 - proceedings of the 12th ACM International Conference on Multimedia},
keywords = {Content Analysis,E-learning,Educational Videos,Video Streaming,Wireless Communications},
pages = {400--403},
title = {{Real-time content analysis and adaptive transmission of lecture videos for mobile applications}},
url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-13444287781{\&}partnerID=tZOtx3y1},
year = {2004}
}
@article{Usach-Molina2010,
abstract = {This paper presents a new content-based method for real-time keyframe selection in H.264 low delay video coding. Based on dynamic thresholds, it is aimed at improving compression efficiency. It has been trained and tested with real sequences taken from movies and commercials, with very different motion and complexity characteristics, and from low to high bitrates, number of frames per second and formats, with a total of 70000 frames and more than 1500 candidate keyframes. Results are presented achieving up to 2.32 dB average peak signal-to-noise ratio improvement, higher than recent methods in the literature, and even processing time gains, preserving the real-time and low delay conditions in real-time coders.},
author = {Usach-Molina, Pau and Sastre, Jorge and Naranjo, Valery and Vergara, Luis and Mu{\~{n}}oz, Joaqun M L{\'{o}}pez},
doi = {10.1109/TCSVT.2010.2051276},
file = {:home/arturo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Usach-Molina et al. - 2010 - Content-based dynamic threshold method for real-time keyframe selecting.pdf:pdf},
isbn = {1051-8215},
issn = {10518215},
journal = {IEEE Transactions on Circuits and Systems for Video Technology},
keywords = {Content-based,H264 video coding,Keyframe selection,Low delay,Real-time},
number = {7},
pages = {982--993},
title = {{Content-based dynamic threshold method for real-time keyframe selecting}},
volume = {20},
year = {2010}
}
@article{Youm2003,
abstract = {In general, the scene change for video indexing could be detected by using an appropriate threshold. However, the single fixed threshold value used in previous researches cannot perform well for all videos equally. In this paper, we propose a dynamic threshold method (DTM) in order to improve the performance of the scene change detection. The proposed method utilizes variable parameters obtained from detected cuts and the distribution of cut intervals in order to change the threshold dynamically. In short, the threshold in DTM is lowered in the area where the chance of cut occurrence is high and raised in the area where the chance of cut occurrence is low. Experimental results indicate that the proposed DTM is both computationally low and robust to false alarm.},
author = {Youm, Sungju and Kim, Woosaeng},
doi = {10.1109/ICME.2003.1221622},
file = {:home/arturo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Youm, Kim - 2003 - Dynamic threshold method for scene change detection.pdf:pdf},
isbn = {0780379659},
issn = {1945788X},
journal = {Proceedings - IEEE International Conference on Multimedia and Expo},
pages = {II337--II340},
title = {{Dynamic threshold method for scene change detection}},
volume = {2},
year = {2003}
}
