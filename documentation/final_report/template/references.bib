@article{Bianco2005,
abstract = {Software routers based on off-the-shelf hardware and open-source operating systems are gaining more and more momentum. The reasons are manifold: first, personal computer (PC) hardware is broadly available at low cost; second, large-scale production and the huge market spur the manufacturers to closely track the improvements made available by Moore's Law; third, open-source software leaves the freedom to study the source code, learn from it, modify it to improve the performance, and tailor its operation to one's own needs. In this paper we focus only on the data plane performance and compare the default Linux IP stack with the Click modular IP stack in terms of the forwarding throughput. The results are surprising and show that a high-end PC is easily able to fit into the multi-Gigabit-per-second routing segment, for a price much lower than commercial routers.},
author = {Bianco, A. and Birke, R. and Bolognesi, D. and Finochietto, J.M. and Galante, G. and Mellia, M. and Prashant, M.L.N.P.P. and Neri, F.},
doi = {10.1109/HPSR.2005.1503186},
file = {:home/arturo/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bianco et al. - 2005 - Click vs. Linux two efficient open-source IP network stacks for software routers.pdf:pdf},
isbn = {0-7803-8924-7},
journal = {HPSR. 2005 Workshop on High Performance Switching and Routing, 2005.},
keywords = {Click modular IP stack,Computer aided manufacturing,Costs,IP networks,Large-scale systems,Linux,Linux IP stack,Microcomputers,Open source hardware,Open source software,Operating systems,Production,data plane performance,forwarding throughput,microcomputers,multiGigabit-per-second routing segment,network operating systems,off-the-shelf hardware,open-source IP network stacks,open-source operating systems,open-source software,personal computer,public domain software,software routers,telecommunication network routing},
pages = {18--23},
title = {{Click vs. Linux: two efficient open-source IP network stacks for software routers}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=1503186},
year = {2005}
}
